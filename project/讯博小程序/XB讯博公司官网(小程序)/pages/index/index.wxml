<template is="head" data="{{app}}" />
<swiper indicator-dots="true" autoplay="true" circular="true" interval="4000" duration="500" class="panel-scroll">
    <block wx:for="{{bannerList}}" wx:key="{{*this}}">
        <swiper-item>
            <image src="{{item}}" mode="aspectFill" class="img"></image>
