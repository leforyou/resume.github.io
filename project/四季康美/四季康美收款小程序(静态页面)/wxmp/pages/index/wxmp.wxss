@charset "UTF-8";
.home .bg {
  background-color: #f4f4f4;
}

.home .fill-info {
  line-height: 90rpx;
}

.home .info-ul .li .title {
  line-height: 80rpx;
  padding-left: 32rpx;
}

.home .info-ul .li .title.not-null {
  position: relative;
}

.home .info-ul .li .title.not-null::before {
  content: '*';
  display: block;
  line-height: 1;
  color: #f00;
  position: absolute;
  top: 50%;
  left: 0rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}

.home .info-ul .li input, .home .info-ul .li textarea {
  box-sizing: border-box;
  padding: 20rpx 32rpx;
  display: block;
  width: 100%;
}

.home .info-ul .li input {
  height: 90rpx;
  line-height: 50rpx;
}

.home .info-ul .li textarea {
  height: 90rpx;
  line-height: 50rpx;
}

.home .service-charge {
  padding: 80rpx 0rpx 30rpx;
}

.home .service-ul .li.active .head .right {
  background-image: url(data:image/png;base64,);
}

.home .service-ul .li.active .content {
  height: auto;
}

.home .service-ul .li .head {
  line-height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.home .service-ul .li .head .right {
  height: 90rpx;
  width: 100rpx;
  position: relative;
  left: 30rpx;
  background-image: url(data:image/png;base64,);
  background-position: center;
  background-repeat: no-repeat;
  background-size: 30rpx;
}

.home .service-ul .li .content {
  height: 0rpx;
  overflow: hidden;
  transition: all 0.3s;
}

.home .service-ul .li .content .content-li {
  position: relative;
  display: flex;
  padding: 22rpx 0rpx;
  /* 未选中的 背景样式 */
  /* 选中后的 背景样式 （红色背景 无边框 可根据UI需求自己修改） */
  /* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
}

.home .service-ul .li .content .content-li .year {
  margin-right: 40rpx;
}

.home .service-ul .li .content .content-li radio .wx-radio-input {
  width: 34rpx;
  height: 34rpx;
  border-radius: 100%;
  position: absolute;
  top: 50%;
  right: -3rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}

.home .service-ul .li .content .content-li radio .wx-radio-input.wx-radio-input-checked {
  /*border: none;*/
  background: #fff;
}

.home .service-ul .li .content .content-li radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 100%;
  /* 圆角 */
  width: 34rpx;
  /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 34rpx;
  /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 34rpx;
  text-align: center;
  font-size: 24rpx;
  /* 对勾大小 30rpx */
  color: #fff;
  /* 对勾颜色 白色 */
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.home .wrap-btn {
  padding-top: 50rpx;
  padding-bottom: 50rpx;
}

.home .wrap-btn .btn {
  line-height: 90rpx;
  background-color: #4bb200;
}
